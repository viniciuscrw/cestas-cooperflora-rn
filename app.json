{
  "expo": {
    "name": "cestascooperflora",
    "description": "Aplicativo para gerenciamento de vendas das cestas de produtos org√¢nicos da cooperativa Cooperflora",
    "slug": "cestascooperflora",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "3.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "splash": {
      "image": "./assets/images/splashscreenv2.0.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "br.pro.bordignon",
      "buildNumber": "3.1.0"
    },
    "android": {
      "package": "br.pro.bordignon",
      "googleServicesFile": "./google-services.json",
      "versionCode": 29,
      "useNextNotificationsApi": true,
      "permissions": [
        "CAMERA",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "ifsp-fn",
            "project": "cestas-cooperflora",
          }
        }
      ]
    },
    "plugins": [
      "sentry-expo"
    ],
    "extra": {
      "eas": {
        "projectId": "0ee452b5-ed8e-48f7-8214-8be5cc32cfa4"
      }
    }
  }
}
