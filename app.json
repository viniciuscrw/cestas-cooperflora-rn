{
  "expo": {
    "name": "cestascooperflora",
    "description": "Aplicativo para gerenciamento de vendas das cestas de produtos org√¢nicos da cooperativa Cooperflora",
    "slug": "cestascooperflora",
    "platforms": ["ios", "android"],
    "version": "3.1.3",
    "orientation": "portrait",
    "backgroundColor": "#ffffff",
    "icon": "./assets/images/icon.png",
    "notification": {
      "icon": "./assets/images/notificationicon.png",
      "color": "#007aff",
      "androidMode": "collapse",
      "androidCollapsedTitle": "New Notification",
      "iosDisplayInForeground": true
    },
    "splash": {
      "image": "./assets/images/splashscreenv2.0.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "br.pro.bordignon",
      "buildNumber": "3.1.3"
    },
    "android": {
      "package": "br.pro.bordignon",
      "googleServicesFile": "./google-services.json",
      "versionCode": 32,
      "permissions": ["CAMERA", "WRITE_EXTERNAL_STORAGE"]
    },
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "ifsp-fn",
            "project": "cestas-cooperflora"
          }
        }
      ]
    },
    "plugins": ["sentry-expo"],
    "extra": {
      "eas": {
        "projectId": "0ee452b5-ed8e-48f7-8214-8be5cc32cfa4"
      }
    }
  }
}
